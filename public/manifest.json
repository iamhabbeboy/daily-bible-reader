{
  "name": "Daily Bible Reader",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Learn daily verses by listing to synthesis voice",
  "browser_action": {
    "default_icon": "church.png",
    "icons": {
      "16": "church.png",
      "48": "church.png",
      "128": "church.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["activeTab"]
}
